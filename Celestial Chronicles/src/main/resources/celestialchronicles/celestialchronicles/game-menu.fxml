<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" stylesheets="@styles/game-menu.css"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="celestialchronicles.celestialchronicles.GameMenu">
    <Button layoutX="1725.0" layoutY="14.0" onAction="#guidebookClicked" prefWidth="181.0" styleClass="simple-button"
            text="Guidebook"/>
    <Button fx:id="level1Button" layoutX="274.0" layoutY="360.0" maxHeight="250.0" maxWidth="250.0"
            onAction="#level1Clicked" prefHeight="186.0" prefWidth="204.0"
            styleClass="star-button" text="1"/>
    <Button fx:id="level2Button" layoutX="548.0" layoutY="360.0" onAction="#level2Clicked" prefHeight="186.0"
            prefWidth="250.0" styleClass="star-button" text="2"/>
    <Button fx:id="level3Button" layoutX="822.0" layoutY="360.0" onAction="#level3Clicked" prefHeight="186.0"
            prefWidth="250.0" styleClass="star-button" text="3"/>
    <Button fx:id="level4Button" layoutX="1096.0" layoutY="360.0" onAction="#level4Clicked" prefHeight="186.0"
            prefWidth="250.0" styleClass="star-button" text="4"/>
    <Button fx:id="level5Button" layoutX="1370.0" layoutY="360.0" onAction="#level5Clicked" prefHeight="186.0"
            prefWidth="250.0" styleClass="star-button" text="5"/>
    <Button layoutX="14.0" layoutY="14.0" onAction="#menuClicked" prefWidth="181.0" styleClass="simple-button"
            text="Menu"/>
    <Button layoutX="1725.0" layoutY="1025.0" onAction="#clearAllLevels" prefWidth="181.0" styleClass="simple-button"
            text="Delete data"/>


    <Label fx:id="lvl1BestTime" layoutX="323.0" layoutY="620.0" prefHeight="41.0" prefWidth="152.0" textFill="WHITE"/>
    <Label fx:id="lvl1BestScore" layoutX="323.0" layoutY="661.0" prefHeight="41.0" prefWidth="152.0" textFill="WHITE"/>
    <Label fx:id="lvl2BestTime" layoutX="597.0" layoutY="620.0" prefHeight="41.0" prefWidth="152.0" textFill="WHITE"/>
    <Label fx:id="lvl3BestTime" layoutX="871.0" layoutY="620.0" prefHeight="41.0" prefWidth="152.0" textFill="WHITE"/>
    <Label fx:id="lvl4BestTime" layoutX="1145.0" layoutY="620.0" prefHeight="41.0" prefWidth="152.0" textFill="WHITE"/>
    <Label fx:id="lvl5BestTime" layoutX="1419.0" layoutY="620.0" prefHeight="41.0" prefWidth="152.0" textFill="WHITE"/>
    <Label fx:id="lvl2BestScore" layoutX="597.0" layoutY="661.0" prefHeight="41.0" prefWidth="152.0" textFill="WHITE"/>
    <Label fx:id="lvl3BestScore" layoutX="871.0" layoutY="661.0" prefHeight="41.0" prefWidth="152.0" textFill="WHITE"/>
    <Label fx:id="lvl4BestScore" layoutX="1145.0" layoutY="661.0" prefHeight="41.0" prefWidth="152.0" textFill="WHITE"/>
    <Label fx:id="lvl5BestScore" layoutX="1419.0" layoutY="661.0" prefHeight="41.0" prefWidth="152.0" textFill="WHITE"/>

</AnchorPane>
